<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_BTS_FORM_LBL_CONFIG_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="field" type="text"
                
            label="COM_BTS_FORM_LBL_CONFIG_FIELD"
            description="COM_BTS_FORM_DESC_CONFIG_FIELD" 
                
                
                
            filter="string" /> 

       <field name="label" type="text"
                
            label="COM_BTS_FORM_LBL_CONFIG_LABEL"
            description="COM_BTS_FORM_DESC_CONFIG_LABEL" 
                
                
                
            filter="string" /> 

       <field name="displayable" type="text" size="40" class="inputbox"
        
            label="COM_BTS_FORM_LBL_CONFIG_DISPLAYABLE"
            description="COM_BTS_FORM_DESC_CONFIG_DISPLAYABLE" 
         default="1" 
        
        
        filter="safehtml" /> 

       <field name="editable" type="text" size="40" class="inputbox"
        
            label="COM_BTS_FORM_LBL_CONFIG_EDITABLE"
            description="COM_BTS_FORM_DESC_CONFIG_EDITABLE" 
        
        
        
        filter="safehtml" /> 

         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
